<% if flash[:notice] %>
  <%= flash[:notice] %>
<% end %>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
<br>
  <%= f.label :email %>
  <%= f.email_field :email %>
<br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
<br>
  <%= f.label :user_name %>
  <%= f.text_field :user_name %>
<br>
  <%= f.submit "Update" %>
<br>
<% end %>
